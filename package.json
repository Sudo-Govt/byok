{\n  "name": "byok",\n  "version": "1.0.0",\n  "description": "Bring Your Own Keys - A comprehensive project management platform",\n  "main": "index.js",\n  "scripts": {\n    "install:all": "npm install && npm run install:backend && npm run install:frontend",\n    "install:backend": "cd backend/shared && npm install && cd ../project-service && npm install",\n    "install:frontend": "cd frontend && npm install",\n    "build": "npm run build:backend && npm run build:frontend",\n    "build:backend": "cd backend/shared && npm run build && cd ../project-service && npm run build",\n    "build:frontend": "cd frontend && npm run build",\n    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",\n    "dev:backend": "cd backend/project-service && npm run dev",\n    "dev:frontend": "cd frontend && npm start",\n    "start": "npm run start:backend",\n    "start:backend": "cd backend/project-service && npm start",\n    "test": "npm run test:backend && npm run test:frontend",\n    "test:backend": "cd backend/shared && npm test && cd ../project-service && npm test",\n    "test:frontend": "cd frontend && npm test",\n    "lint": "npm run lint:backend && npm run lint:frontend",\n    "lint:backend": "cd backend/shared && npm run lint && cd ../project-service && npm run lint",\n    "lint:frontend": "cd frontend && npm run lint",\n    "clean": "npm run clean:backend && npm run clean:frontend",\n    "clean:backend": "cd backend/shared && npm run clean && cd ../project-service && npm run clean",\n    "clean:frontend": "cd frontend && npm run build && rm -rf frontend/build"\n  },\n  "devDependencies": {\n    "concurrently": "^8.2.0"\n  },\n  "keywords": [\n    "project-management",\n    "full-stack",\n    "typescript",\n    "react",\n    "node.js",\n    "byok"\n  ],\n  "author": "BYOK Team",\n  "license": "MIT",\n  "repository": {\n    "type": "git",\n    "url": "https://github.com/Sudo-Govt/byok"\n  },\n  "engines": {\n    "node": ">=18.0.0",\n    "npm": ">=8.0.0"\n  },\n  "workspaces": [\n    "backend/shared",\n    "backend/project-service",\n    "frontend"\n  ]\n}